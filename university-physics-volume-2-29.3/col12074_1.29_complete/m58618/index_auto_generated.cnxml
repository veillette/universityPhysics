<para id="fs-id1171241192710"><emphasis effect="bold">Trigonometry</emphasis></para>
<para id="fs-id1171241320523"><emphasis effect="italics">Trigonometric Identities</emphasis></para>
<list id="fs-id1171241111731" list-type="enumerated" number-style="arabic">
<item><m:math><m:mrow><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mtext>/</m:mtext><m:mtext>csc</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mtext>/</m:mtext><m:mtext>sec</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>tan</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mtext>/</m:mtext><m:mtext>cot</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>sin</m:mtext><m:mrow><m:mo>(</m:mo><m:mrow><m:msup><m:mrow><m:mn>90</m:mn></m:mrow><m:mn>0</m:mn></m:msup><m:mo>−</m:mo><m:mi>θ</m:mi></m:mrow><m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>cos</m:mtext><m:mrow><m:mo>(</m:mo><m:mrow><m:msup><m:mrow><m:mn>90</m:mn></m:mrow><m:mn>0</m:mn></m:msup><m:mo>−</m:mo><m:mi>θ</m:mi></m:mrow><m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>tan</m:mtext><m:mrow><m:mo>(</m:mo><m:mrow><m:msup><m:mrow><m:mn>90</m:mn></m:mrow><m:mn>0</m:mn></m:msup><m:mo>−</m:mo><m:mi>θ</m:mi></m:mrow><m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mtext>cot</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:msup><m:mrow><m:mtext>sin</m:mtext></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>+</m:mo><m:msup><m:mrow><m:mtext>cos</m:mtext></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>1</m:mn></m:mrow></m:math></item>
<item><m:math><m:mrow><m:msup><m:mrow><m:mtext>sec</m:mtext></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>−</m:mo><m:msup><m:mrow><m:mtext>tan</m:mtext></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>1</m:mn></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>tan</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mtext>/</m:mtext><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>sin</m:mtext><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>α</m:mi><m:mo>±</m:mo><m:mi>β</m:mi></m:mrow><m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi><m:mspace width="0.2em"/><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi><m:mo>±</m:mo><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi><m:mspace width="0.2em"/><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>cos</m:mtext><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>α</m:mi><m:mo>±</m:mo><m:mi>β</m:mi></m:mrow><m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi><m:mspace width="0.2em"/><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi><m:mo>∓</m:mo><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi><m:mspace width="0.2em"/><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>tan</m:mtext><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>α</m:mi><m:mo>±</m:mo><m:mi>β</m:mi></m:mrow><m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac><m:mrow><m:mtext>tan</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi><m:mo>±</m:mo><m:mtext>tan</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi></m:mrow><m:mrow><m:mn>1</m:mn><m:mo>∓</m:mo><m:mtext>tan</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi><m:mspace width="0.2em"/><m:mtext>tan</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi></m:mrow></m:mfrac></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mn>2</m:mn><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mspace width="0.2em"/><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mn>2</m:mn><m:mi>θ</m:mi><m:mo>=</m:mo><m:msup><m:mrow><m:mtext>cos</m:mtext></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>−</m:mo><m:msup><m:mrow><m:mtext>sin</m:mtext></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mspace width="0.2em"/><m:msup><m:mrow><m:mtext>cos</m:mtext></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="0.2em"/><m:mi>θ</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>−</m:mo><m:mn>2</m:mn><m:mspace width="0.2em"/><m:msup><m:mrow><m:mtext>sin</m:mtext></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="0.2em"/><m:mi>θ</m:mi></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi><m:mo>+</m:mo><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mspace width="0.2em"/><m:mtext>sin</m:mtext><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>α</m:mi><m:mo>+</m:mo><m:mi>β</m:mi></m:mrow><m:mo>)</m:mo></m:mrow><m:mtext>cos</m:mtext><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>α</m:mi><m:mo>−</m:mo><m:mi>β</m:mi></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow></m:math></item>
<item><m:math><m:mrow><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi><m:mo>+</m:mo><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mspace width="0.2em"/><m:mtext>cos</m:mtext><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>α</m:mi><m:mo>+</m:mo><m:mi>β</m:mi></m:mrow><m:mo>)</m:mo></m:mrow><m:mtext>cos</m:mtext><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>α</m:mi><m:mo>−</m:mo><m:mi>β</m:mi></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow></m:math></item>
</list>
<para id="fs-id1171241012787"><emphasis effect="italics">Triangles</emphasis></para>
<list id="fs-id1171241129165" list-type="enumerated" number-style="arabic">
<item>Law of sines: <m:math><m:mrow><m:mfrac><m:mi>a</m:mi><m:mrow><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>α</m:mi></m:mrow></m:mfrac><m:mo>=</m:mo><m:mfrac><m:mi>b</m:mi><m:mrow><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>β</m:mi></m:mrow></m:mfrac><m:mo>=</m:mo><m:mfrac><m:mi>c</m:mi><m:mrow><m:mtext>sin</m:mtext><m:mspace width="0.2em"/><m:mi>γ</m:mi></m:mrow></m:mfrac></m:mrow></m:math></item>
<item>Law of cosines: <m:math><m:mrow><m:msup><m:mi>c</m:mi><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:msup><m:mi>a</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:msup><m:mi>b</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>2</m:mn><m:mi>a</m:mi><m:mi>b</m:mi><m:mspace width="0.2em"/><m:mtext>cos</m:mtext><m:mspace width="0.2em"/><m:mi>γ</m:mi></m:mrow></m:math><newline/>
<media id="fs-id1171241119734" alt="Figure shows a triangle with three dissimilar sides labeled a, b and c. All three angles of the triangle are acute angles. The angle between b and c is alpha, the angle between a and c is beta and the angle between a and b is gamma.">
<image mime-type="image/jpeg" src="CNX_UPhysics_00_EE_Triangle1_img.jpg"/>
</media></item>
<item>Pythagorean theorem: <m:math><m:mrow><m:msup><m:mi>a</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:msup><m:mi>b</m:mi><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:msup><m:mi>c</m:mi><m:mn>2</m:mn></m:msup></m:mrow></m:math><newline/>
<media id="fs-id1171241024569" alt="Figure shows a right triangle. Its three sides are labeled a, b and c with c being the hypotenuse. The angle between a and c is labeled theta.">
<image mime-type="image/jpeg" src="CNX_UPhysics_00_EE_Triangle2_img.jpg"/>
</media></item>
</list>
<para id="fs-id1171241190802"><emphasis effect="bold">Series expansions</emphasis></para>
<list id="fs-id1171241016910" list-type="enumerated" number-style="arabic">
<item>Binomial theorem: <m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mi>n</m:mi></m:msup><m:mo>=</m:mo><m:msup><m:mi>a</m:mi><m:mi>n</m:mi></m:msup><m:mo>+</m:mo><m:mi>n</m:mi><m:msup><m:mi>a</m:mi><m:mrow><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn></m:mrow></m:msup><m:mi>b</m:mi><m:mo>+</m:mo><m:mfrac><m:mrow><m:mi>n</m:mi><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow><m:msup><m:mi>a</m:mi><m:mrow><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>2</m:mn></m:mrow></m:msup><m:msup><m:mi>b</m:mi><m:mn>2</m:mn></m:msup></m:mrow><m:mrow><m:mn>2</m:mn><m:mtext>!</m:mtext></m:mrow></m:mfrac><m:mo>+</m:mo><m:mfrac><m:mrow><m:mi>n</m:mi><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>2</m:mn></m:mrow><m:mo>)</m:mo></m:mrow><m:msup><m:mi>a</m:mi><m:mrow><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>3</m:mn></m:mrow></m:msup><m:msup><m:mi>b</m:mi><m:mn>3</m:mn></m:msup></m:mrow><m:mrow><m:mn>3</m:mn><m:mtext>!</m:mtext></m:mrow></m:mfrac><m:mo>+</m:mo><m:mtext>···</m:mtext></m:mrow></m:math></item>
